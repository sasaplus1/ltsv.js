{
  "name": "ltsv",
  "description": "LTSV parser, formatter, validator and TransformStream",
  "version": "2.0.0",
  "author": "sasa+1 <sasaplus1@gmail.com>",
  "browser": "./dist/umd/ltsv.js",
  "devDependencies": {
    "@rollup/plugin-alias": "^5.0.0",
    "@tsconfig/recommended": "^1.0.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "^12.7.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-node": "^11.1.0",
    "espower-typescript": "^10.0.0",
    "husky": "^8.0.0",
    "is-ci": "^3.0.0",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^2.0.1",
    "karma-rollup-preprocessor": "^7.0.7",
    "lint-staged": "^13.0.0",
    "mocha": "^9.1.1",
    "npm-run-all": "^4.1.5",
    "power-assert": "^1.6.1",
    "prettier": "^2.4.0",
    "rollup": "^2.56.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript": "^1.0.1",
    "typedoc": "^0.24.0",
    "typescript": "^4.0.2"
  },
  "engines": {
    "node": ">=8.3"
  },
  "files": [
    "dist/**/*"
  ],
  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.mjs",
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/sasaplus1/ltsv.js.git"
  },
  "scripts": {
    "build": "run-p -l build:*",
    "build-documents": "typedoc",
    "build:cjs": "tsc",
    "build:esm": "rollup -c --environment build:esm",
    "build:umd": "rollup -c --environment build:umd",
    "fix": "run-s fix:*",
    "fix:eslint": "eslint --fix .",
    "fix:prettier": "prettier --write './**/*.+(js|md|ts|yml)'",
    "fixpack": "npx fixpack",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint --cache .",
    "lint:prettier": "prettier --check './**/*.+(js|md|ts|yml)'",
    "lint:tsc": "tsc --noEmit",
    "prepare": "is-ci || husky install",
    "pretest": "run-s build:cjs",
    "pretest:browser": "run-s build:umd",
    "test": "mocha",
    "test:browser": "karma start --single-run"
  },
  "sideEffects": false,
  "types": "./dist/cjs/index.d.ts"
}
